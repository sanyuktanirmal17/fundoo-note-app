{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {      
         "semi":  ["error", "always"],
         "linebreak-style": 0,
         "no-console": 0,
         "no-regex-spaces": 0

    }
}
